<%= form_for recipe do |f| %>
  Title: <%= f.text_field :title %><br>
  Ingredients: <%= f.fields_for :ingredients do |ingredient| %><br>
    	  
    Name: <%= ingredient.text_field :name %><br>
   			
   
    Quantity: <%= ingredient.text_field :quantity %><br>
 
  <% end %>
 <%= f.submit %>
<% end %>
